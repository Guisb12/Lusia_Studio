# Environment
ENVIRONMENT=development

# B2B Supabase (main database)
SUPABASE_URL_B2B=https://your-b2b-project.supabase.co
SUPABASE_KEY_B2B=your-b2b-anon-key
SUPABASE_SERVICE_KEY_B2B=your-b2b-service-key

# B2C Supabase (content library - READ ONLY)
SUPABASE_URL_B2C=https://your-b2c-project.supabase.co
SUPABASE_SERVICE_KEY_B2C=your-b2c-service-key

# OpenAI (for AI features)
OPENAI_API_KEY=sk-...

# App signing secret for enrollment tokens
APP_AUTH_SECRET=replace-with-strong-random-secret
ENROLLMENT_TOKEN_TTL_SECONDS=604800
